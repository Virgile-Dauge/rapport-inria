	keywords = {attitude control, capture points concept, central pattern generation, elliptic trajectory generation, HyQ hydraulically actuated quadruped robot, Jacobian matrices, legged locomotion, mechanical stability, mechanical variables control, motion control, null space based attitude control, push recovery algorithm, reactive controller framework, robot stability control, robot trunk posture, Robustness, robust quadrupedal locomotion, state estimation, task space CPG-based trajectory generation, terrain irregularities, trajectory control, trajectory tracking errors},
	pages = {2554--2561},
	file = {IEEE Xplore Abstract Record:/home/virgile/.mozilla/firefox/j5cafxwk.default/zotero/storage/JQ8UWWUV/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/virgile/.mozilla/firefox/j5cafxwk.default/zotero/storage/WHIDJXHU/Barasuol et al. - 2013 - A reactive controller framework for quadrupedal lo.pdf:application/pdf}
