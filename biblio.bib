	pages = {2171--2176 Vol.3},
	file = {IEEE Xplore Abstract Record:/home/virgile/.mozilla/firefox/j5cafxwk.default/zotero/storage/WURCTH8G/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/virgile/.mozilla/firefox/j5cafxwk.default/zotero/storage/A7ZH7PF8/Kamikawa et al. - 2004 - Omni-directional gait of multi-legged rescue robot.pdf:application/pdf}
