	author = {Chen, Jie and Liu, Yubin and Zhao, Jie and Zhang, He and Jin, Hongzhe},
	month = jan,
	year = {2014},
	keywords = {biological movement principle, biomimetic design, Gauss pseudospectral method, Legged robot, optimal control},
